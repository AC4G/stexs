{
  "name": "stexs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "turbo run build",
    "build:main": "turbo run build:main",
    "build:module": "turbo run build:module",
    "lint": "turbo run lint",
    "dev": "dotenv -- turbo run dev",
    "clean": "turbo run clean",
    "clean:logs": "turbo run clean:logs",
    "type-check": "turbo run type-check",
    "test": "dotenv -- turbo run test",
    "test:coverage": "turbo run test:coverage",
    "test:watch": "turbo run test:watch",
    "test:db": "pg_prove --host localhost --dbname postgres --username postgres apps/db/tests/*.sql apps/db/tests/**/**/*.sql",
    "format": "prettier --write '{apps,packages}/**/*.ts'"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "devDependencies": {
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "turbo": "^1.10.16"
  },
  "dependencies": {
    "@turbo/gen": "^1.10.16",
    "autoprefixer": "^10.4.16",
    "dotenv-cli": "^7.3.0"
  },
  "engines": {
    "npm": ">=10.1.0",
    "node": ">=20.8.0"
  },
  "packageManager": "pnpm@8.9.2"
}
