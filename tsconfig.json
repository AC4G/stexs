{
    "include": ["**/*.ts", "env-config.ts"],
    "compilerOptions": {
        "outDir": "./dist",
        "module": "CommonJS",
        "target": "ES2019",
        "strict": true,
        "allowJs": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "forceConsistentCasingInFileNames": true,
        "baseUrl": ".",
        "paths": {
        "~/*": ["./app/*"]
        },
        "composite": true
    }
}
